{% extends 'base.html.twig' %}

{% block title %}TournamentReports index{% endblock %}

{% block body %}
    <h1>TournamentReports index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tournament_report in tournament_reports %}
            <tr>
                <td>{{ tournament_report.id }}</td>
                <td>
                    <a href="{{ path('app_tournament_reports_show', {'id': tournament_report.id}) }}">show</a>
                    <a href="{{ path('app_tournament_reports_edit', {'id': tournament_report.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_tournament_reports_new') }}">Create new</a>
{% endblock %}
